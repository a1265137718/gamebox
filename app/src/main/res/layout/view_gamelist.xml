<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

    <com.gamebox_idtkown.views.widgets.GBTitleView2
        android:id="@+id/titleView"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentLeft="true"></com.gamebox_idtkown.views.widgets.GBTitleView2>

    <com.gamebox_idtkown.views.widgets.GBGridView
        android:id="@+id/gamelist"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/titleView"
        android:numColumns="4"></com.gamebox_idtkown.views.widgets.GBGridView>
</RelativeLayout>